{
  "name": "auth-server",
  "version": "1.0.0",
  "description": "Auth Server follows OAuth 2 and OpenID Connect protocol",
  "main": "bin/www",
  "scripts": {
    "build": "rimraf dist/ && babel app -d dist",
    "start": "nodemon -i ./dist -x \"npm run build && node ./dist/server.js\"",
    "test": "jest",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trongbq/auth-server.git"
  },
  "keywords": [
    "auth-server",
    "oauth2",
    "openid_connect"
  ],
  "author": "Trong Bui",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trongbq/auth-server/issues"
  },
  "homepage": "https://github.com/trongbq/auth-server#readme",
  "dependencies": {
    "express": "^4.17.1",
    "mongodb": "^4.1.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.1.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6"
  }
}
